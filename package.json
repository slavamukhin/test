{
  "name": "api-management-ui",
  "version": "1.0.0",
  "description": "UI for uzcard",
  "main": "index.tsx",
  "scripts": {
    "start": "webpack serve --mode development",
    "build:prod": "webpack --mode production",
    "lint": "eslint --fix 'src/*.{js,jsx,ts,tsx}'",
    "test": "echo 'test'",
    "format": "prettier --write 'src/**/*.{json,js,ts,tsx}'",
    "clean:code": "del src/interfaces/** '!src/interfaces/model' '!src/interfaces/index.ts'; rm -rf src/interfaces/.openapi-generator; rm -rf src/interfaces/.openapi-generator-ignore; npm run format",
    "codegen": "rm -rf src/interfaces/model; openapi-generator-cli generate -i https://apimgmt-uzcards.dbp.inno.tech/public/v3/api-docs -g typescript-inversify -o src/interfaces --additional-properties=supportsES6=true,withInterfaces=true; npm run clean:code"
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "author": "Mukhin V.A.",
  "browserslist": "> 0.25%, not dead",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
