    "@babel/preset-typescript": "^7.16.5",
    "@svgr/webpack": "^6.1.2",
    "@types/js-cookie": "^3.0.1",
    "@types/styled-components": "^5.1.18",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^10.2.0",
    "del-cli": "^4.0.1",
